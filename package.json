{
  "name": "new-copenhagenjs.dk",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "npm run prepare && next build && next export",
    "deploy": "npm run build && firebase deploy",
    "dev": "npm run prepare && next",
    "fix-lint": "npx prettier --write --single-quote --no-semi next.config.js 'components/**/*.js' 'pages/*.js' '_posts/*.json' 'utils/*.js' 'data/*.js'",
    "lint": "npx prettier -l --single-quote --no-semi next.config.js 'components/**/*.js' 'pages/*.js' '_posts/*.json' 'utils/*.js' 'data/*.js'",
    "prepare": "npm run process-archive && cp -R _posts/ static/posts",
    "process-archive": "mkdirp pages/archive && node utils/processmd",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "npm run build-storybook && npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "apollo-boost": "^0.4.3",
    "cross-fetch": "^3.0.4",
    "front-matter": "^3.0.2",
    "graphql": "^14.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "marked": "^0.7.0",
    "mkdirp": "^0.5.1",
    "next": "^9.0.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@storybook/react": "^5.1.10",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@babel/core": "^7.5.5",
    "babel-loader": "^8.0.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
